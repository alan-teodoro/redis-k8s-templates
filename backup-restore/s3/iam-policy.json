{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "RedisEnterpriseS3BackupAccess",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:DeleteObject",
        "s3:ListBucket",
        "s3:GetBucketLocation"
      ],
      "Resource": [
        "arn:aws:s3:::redis-backups",
        "arn:aws:s3:::redis-backups/*"
      ]
    }
  ]
}

